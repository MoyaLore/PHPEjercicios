<?php
$numero1 =rand(0,10);
$numero2 =rand(0,10);
$numero3 =rand(0,10);
$numeros=array ($numero1, $numero2, $numero3 );

echo "El valor del primer número es $numero1"."\n";
echo "El valor del primer número es $numero2"."\n";
echo "El valor del primer número es $numero3"."\n";

if ($numero1 == $numero2 && $numero2 == $numero3 && $numero3 == $numero1){
echo "Son iguales!";
} else {
rsort($numeros);
print_r($numeros);
}
?> 

